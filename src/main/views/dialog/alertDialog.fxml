<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane styleClass="pageContent" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.views.dialog.alertDialogController">
   <children>
      <AnchorPane id="dialogContent">
         <children>
            <VBox alignment="CENTER" prefWidth="450.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label fx:id="subject" style="-fx-font-weight: bold;" styleClass="title" text="Confirm Message">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="message" alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" styleClass="text" text="text" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="15.0" />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Button styleClass="function-button" minWidth="120.0" mnemonicParsing="false" onAction="#clickOk" prefHeight="45.0" text="OK">
                           <padding>
                              <Insets left="20.0" right="20.0" />
                           </padding></Button>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="50.0" left="60.0" right="60.0" top="60.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@../../assests/stylesheets/style.css" />
      <URL value="@../../assests/stylesheets/dialog.css" />
   </stylesheets>
</AnchorPane>
