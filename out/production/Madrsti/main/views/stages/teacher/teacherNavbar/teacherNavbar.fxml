<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="navBG" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.views.stages.teacher.teacherNavbar.teacherNavbarController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="640.0" prefWidth="250.0">
         <children>
            <Button fx:id="teacherShowHome" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#teacherShowHome" prefWidth="100.0" styleClass="active" text="HOME" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="25.0" top="25.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="HOME" size="25" styleClass="icon" />
               </graphic></Button>
            <Button fx:id="teacherShowProfile" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#teacherShowProfile" prefWidth="100.0" text="PROFILE" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="25.0" top="25.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="USER" size="25" styleClass="icon" />
               </graphic></Button>
         </children>
         <padding>
            <Insets top="30.0" />
         </padding>
      </VBox>
   </children>
    <stylesheets>
        <URL value="@../../../../assests/stylesheets/navbar.css" />
    </stylesheets>
</AnchorPane>
