<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="navBG" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.views.stages.admin.adminNavbar.adminNavbarController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="250.0">
         <children>
            <Button fx:id="adminShowHome" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#adminShowHome" prefWidth="110.0" styleClass="active" text="HOME" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="30.0" top="30.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="HOME" styleClass="icon" />
               </graphic></Button>
            <Button fx:id="adminShowStaff" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#adminShowStaff" prefWidth="110.0" text="STAFF" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="30.0" top="30.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="BRIEFCASE" size="25" styleClass="icon" />
               </graphic></Button>
            <Button fx:id="adminShowStudents" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#adminShowStudents" prefWidth="110.0" text="STUDENTS" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="30.0" top="30.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="USERS" size="25" styleClass="icon" />
               </graphic></Button>
            <Button fx:id="adminShowResults" alignment="CENTER" contentDisplay="TOP" graphicTextGap="15.0" mnemonicParsing="false" onAction="#adminShowResults" prefWidth="110.0" text="RESULTS" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="30.0" top="30.0" />
               </padding>
               <graphic>
                  <FontAwesomeIconView glyphName="FILE_TEXT_ALT" size="25" styleClass="icon" />
               </graphic></Button>
         </children>
         <padding>
            <Insets top="50.0" />
         </padding>
      </VBox>
   </children>
    <stylesheets>
        <URL value="@../../../../assests/stylesheets/navbar.css" />
    </stylesheets>
</AnchorPane>
